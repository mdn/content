{
  "problemMatcher": [
    {
      "owner": "log-url-issues",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(ERROR|WARN|INFO):(.+):(\\d+):(\\d+):(.+)$",
          "severity": 1,
          "file": 2,
          "line": 3,
          "column": 4,
          "message": 5
        }
      ]
    }
  ]
}
